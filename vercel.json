{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "./dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.cia-oswald-tracker.vercel.app/"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/public/index.html"
    }
  ]
}